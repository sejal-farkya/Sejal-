#include<iostream >
#include<vector >
using namespace std;
int main()
{
int M,N;
vector<int>v,t;
cin>>M>>N;
for(int i=0;i<N;++i)
{
 int x;
 cin>>x;
 v.push_back(x);
}
int psum=0;
for(int j=N;j>0&&psum!=M;--j)
{
 int csum=0;
 vector<int>f;
 for(int i=j-1;i>=0;--i)
 if(v[i]+csum<=M)
 {
  f.push_back(i);
  csum+=v[i];
 }
  if(csum>psum)
  {
   psum=csum;
   t=f;
  }
 }
 cout<<t.size()<<"\n";
 for(int i=t.size()-1;i>=0;--i)
 cout<<t[i]<<" ";
 return 0;
 }
